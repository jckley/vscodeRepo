// CHILD (Contact) to PARENT(Territorio)

SELECT 
    Id,
    Name,
    Territorio_Administrativo__r.Id,
    Territorio_Administrativo__r.Name,
    Territorio_Administrativo__r.Nivel_Administrativo__c,
    Territorio_Administrativo__r.Location_UUID__c,
    Territorio_Administrativo__r.ParentId__r.Name
FROM Contact
WHERE Document_Number__c ='35268320'

